<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.emona.edit.feature"
      label="Emona Editor Feature"
      version="0.1.0">

   <description>
      Emona is a editor for nagios configuration files.
It is based on Xtext and provides syntax validation, formatting
and proposals.
   </description>

   <copyright>
      Copyright (c) 2011 Martin Stockhammer &lt;m.stockhammer@web.de&gt;
All rights reserved.
   </copyright>

   <license url="http://www.eclipse.org/legal/epl-v10.html">
      This program and the accompanying materials are made available under the terms of the Eclipse Public License v1.0 which accompanies this distribution, and is available at 
http://www.eclipse.org/legal/epl-v10.html
   </license>

   <requires>
      <import plugin="org.eclipse.osgi"/>
      <import plugin="org.eclipse.xtext"/>
      <import plugin="org.eclipse.xtext.util"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.emf.common"/>
      <import plugin="org.antlr.runtime"/>
      <import plugin="ch.qos.logback.classic"/>
      <import plugin="ch.qos.logback.core"/>
      <import plugin="org.apache.commons.logging"/>
      <import plugin="org.apache.log4j"/>
      <import plugin="org.slf4j.api"/>
      <import plugin="org.eclipse.xtext.ui"/>
      <import plugin="org.eclipse.ui.editors" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.ide" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.xtext.ui.shared"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.xtext.builder"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.junit"/>
      <import plugin="javax.activation"/>
   </requires>

   <plugin
         id="javax.activation"
         download-size="0"
         install-size="0"
         version="1.1.0.qualifier"
         unpack="false"/>

   <plugin
         id="javax.mail"
         download-size="0"
         install-size="0"
         version="1.4.0.qualifier"
         unpack="false"/>

   <plugin
         id="org.emona.edit"
         download-size="0"
         install-size="0"
         version="0.1.0"
         unpack="false"/>

   <plugin
         id="org.emona.edit.ui"
         download-size="0"
         install-size="0"
         version="0.1.0"
         unpack="false"/>

   <plugin
         id="org.emona.model"
         download-size="0"
         install-size="0"
         version="0.1.0"
         unpack="false"/>

</feature>
